{
  "name": "dota-pro-tracker-parser",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "bin": {
    "dota-tracker": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "bun --watch index.js",
    "build": "bun build index.js --outfile dist/dota-tracker --target node --minify",
    "build:exe": "bun build index.js --compile --outfile dist/dota-tracker",
    "build:exe:win": "bun build index.js --compile --target=bun-windows-x64 --outfile dist/dota-tracker-windows.exe",
    "build:exe:linux": "bun build index.js --compile --target=bun-linux-x64 --outfile dist/dota-tracker-linux",
    "build:exe:mac": "bun build index.js --compile --target=bun-darwin-x64 --outfile dist/dota-tracker-macos",
    "build:exe:mac-arm": "bun build index.js --compile --target=bun-darwin-arm64 --outfile dist/dota-tracker-macos-arm64",
    "build:all": "npm run build:exe:win && npm run build:exe:linux && npm run build:exe:mac && npm run build:exe:mac-arm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "CLI tool for tracking Dota 2 pro player statistics",
  "keywords": ["dota2", "pro-tracker", "cli", "statistics"],
  "dependencies": {}
}
